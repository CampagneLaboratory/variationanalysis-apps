{
  "name": "genotypeSbi-to-tensors-2",
  "title": "Convert Genotype .sbi to Tensors",
  "summary": "GenotypeSbiToTensors",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "GenotypeSBI",
      "label": "Genotype Sequence Base Information",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*.sbi",
        "*.sbip"
      ],
      "help": ""
    },
    {
      "name": "FeatureMapper",
      "label": "Feature Mapper",
      "class": "string",
      "default": "org.campagnelab.dl.genotype.mappers.GenotypeMapperV37",
      "optional": false,
      "help": ""
    },
    {
      "name": "SamplingRate",
      "label": "Sampling Rate",
      "class": "float",
      "optional": false,
      "help": ""
    },
    {
      "name": "SampleName",
      "label": "Sample Name",
      "class": "string",
      "optional": false,
      "help": ""
    },
    {
      "name": "Annotation",
      "label": "File with Annotations",
      "class": "file",
      "patterns": [
        "*.vcf.gz"
      ],
      "help": ""
    },
    {
      "name": "Ploidy",
      "label": "Ploidy",
      "class": "int",
      "default": 2,
      "optional": false,
      "help": "The organism ploidy (2 for humans, more for some plants)."
    },
    {
      "name": "GenomicContextLength",
      "label": "Genomic Context Length",
      "class": "int",
      "default": 29,
      "optional": false,
      "help": "ength of genomic context to use around site, in mapped features."
    },
    {
      "name": "LabelSmoothingEpsilon",
      "label": "Label Smoothing Epsilon",
      "class": "float",
      "default": 0,
      "optional": false,
      "help": "Value of epsilon for label smoothing. Zero (default) is no smoothing."
    }
  ],
  "outputSpec": [
    {
      "name": "Tensors",
      "label": "Tensor File",
      "class": "array:file",
      "patterns": [
        "*.vec",
        "*.vecp"
      ],
      "help": ""
    }
  ],
  "developers": [
    "org-campagnelab"
  ],
  "authorizedUsers": [
    "org-campagnelab"
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 48
      }
    },
    "interpreter": "bash",
    "release": "14.04",
    "distribution": "Ubuntu",
    "file": "src/GenotypeSbiToTensors.sh"
  },
  "access": {
    "network": [
      "*"
    ]
  },
  "regionalOptions": {
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_x2"
        }
      }
    }
  }
}
